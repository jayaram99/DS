class Node:
   def __init__(self, data=None):
      self.data = data
      self.next = None

class SLinkedList:
   def __init__(self):
      self.head = None
      
   def listprint(self):
       printval = self.head
       while printval is not None:
           print (printval.data)
           printval = printval.next

list1 = SLinkedList()
list1.head = Node("Mon")
e2 = Node("Tue")
e3 = Node("Wed")
# Link first Node to second node
list1.head.next = e2

# Link second Node to third node
e2.next = e3
list1.listprint()
